<ul class="nav nav-tabs">
  <li> <%= link_to tag_icon(:list, pat(:list)), url(:rooms, :index) %></li>
  <li> <%= link_to tag_icon(:plus, pat(:new)), url(:rooms, :new) %></li>
  <li class="active"> <%= link_to tag_icon(:edit, pat(:edit)), url(:rooms, :edit, :id => @room.id) %></li>
</ul>
<div class="tabs-content">
  <% form_for :room, url(:rooms, :update, :id => @room.id), :method => :put, :class => 'form-horizontal' do |f| %>
    <%= partial 'rooms/form', :locals => { :f => f } %>
  <% end %>
</div>
